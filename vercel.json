{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "(.*)",
            "destination": "/"
        }
    ]
}